<% form_for @user, :url => users_path do |form| %>

  <%= form.label :email %>
  <%= form.text_field :email %><br/>
  <%= form.label :password %>
  <%= form.password_field :password %><br/>
  <%= form.label :password_confirmation %>
  <%= form.password_field :password_confirmation %><br/>
  <%= form.submit 'sign up'%>
<% end %>